<?xml version="1.0" encoding="UTF-8" ?>
<ftp-config>
    <ftp-server>192.168.102.117</ftp-server>
    <ftp-port>21</ftp-port>
    <ftp-user>userftp</ftp-user>
    <ftp-passwd>userftp</ftp-passwd>
    <!--This path is the FTP Server source address.-->
    <data-file-source-location>/home/userftp/ftpdir/down</data-file-source-location>
    <!--This path is the FTP Server upload file address.-->
    <data-file-target-location>/home/userftp/ftpdir/up</data-file-target-location>
    <!--This path is the storage address for the file from the FTP Server.-->
    <data-file-save-location>/home/userftp/data</data-file-save-location>
    <scan-time>60000</scan-time>
</ftp-config>